.thumbs {
    width: 178px;
}
.thumbs__item--active {
    outline: 3px solid #f29400;
}

.thumb__link {
    display: block;
    height: 100%;
}

.thumbs__pic {
    width: 100%;
    height: 100%;
    object-fit: cover;
    &:hover {
        transition: .5s;
        outline: 3px solid #f29400;
    }
}