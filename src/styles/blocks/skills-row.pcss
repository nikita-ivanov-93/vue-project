.skills-row {
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    margin-bottom: 36px;
    .skills-row__title {
        font-weight: 700;
        font-size: 24px;
        margin-bottom: 23px;
    }
    .skills-row__list {
